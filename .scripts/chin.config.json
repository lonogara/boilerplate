{
  "DEVELOPMENT": {
    "put": "./frame",
    "out": "./_local",
    "process": {},
    "weirs": [
      ["./onerror.js", { "js": ["EmbedJs", "utf8"] }]
    ],
    "ignore": {
      "targets": [
        "./favicons.png",
        "./ganalytics.js",
        "./robots.txt",
        "./sitemap.xml"
      ]
    }
  },

  "PRODUCTION:LIGHT": {
    "put": "./frame",
    "out": "./_site",
    "process": {},
    "weirs": [
      ["./onerror.js", { "js": ["EmbedJs", "utf8"] }]
    ],
    "ignore": {
      "targets": [
        "./favicons.png",
        "./ganalytics.js",
        "./robots.txt",
        "./sitemap.xml"
      ]
    }
  },

  "PRODUCTION:MASTER": {
    "put": "./frame",
    "out": "./_site",
    "process": {},
    "weirs": [
      ["./ganalytics.js", { "js": ["EmbedJs", "utf8"] }],
      ["./favicons.png", { "png": "EmbedFavicons" }]
    ],
    "ignore": {
      "targets": [
        "./onerror.js"
      ]
    }
  },

  "INKSCHIN": {
    "put": "./ikc",
    "out": "./frame",
    "weirs": [
      ["./favicons.svg", { "svg": ["InkPngWhite", null, "stream"]}]
    ]
  }
}
